		<dc:title id="title">The Autobiography of an Ex-Colored Man</dc:title>
		<meta property="file-as" refines="#title">Autobiography of an Ex-Colored Man, The</meta>
		<dc:subject id="subject-1">African American men -- Fiction</dc:subject>
		<dc:subject id="subject-2">Racially mixed people -- Fiction</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
		<meta property="term" refines="#subject-1">sh2007100672</meta>
		<meta property="authority" refines="#subject-2">LCSH</meta>
